<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	
	<dubbo:application name="${cherong.service.application}" />
	<!-- 使用zookeeper注册中心暴露服务地址  -->
	<dubbo:registry address="${dubbo.cherong.address}" />
	
	<!-- 声明需要引用的服务接口  -->

	<dubbo:reference 
		id="userService" 
		interface="com.cherong.service.user.service.UserService" 
		group="${dubbo.group.cherong.service.user}" 
		version="${dubbo.version.cherong.service.user}" 
		timeout="${dubbo.service.timeout}" 
		retries="${dubbo.service.retries}"/>

</beans>
